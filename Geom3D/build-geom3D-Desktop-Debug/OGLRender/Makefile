	@test -d .tmp/OGLRender1.0.0 || mkdir -p .tmp/OGLRender1.0.0
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/OGLRender1.0.0/ && $(COPY_FILE) --parents ../../TP_Geom3D/OGLRender/shaderprogram.h ../../TP_Geom3D/OGLRender/shader.h ../../TP_Geom3D/OGLRender/shaderprogramcolor.h ../../TP_Geom3D/OGLRender/shaderprogramflat.h ../../TP_Geom3D/OGLRender/shaderprogramphong.h .tmp/OGLRender1.0.0/ && $(COPY_FILE) --parents ../../TP_Geom3D/OGLRender/shader.cpp ../../TP_Geom3D/OGLRender/shaderprogram.cpp ../../TP_Geom3D/OGLRender/shaderprogramcolor.cpp ../../TP_Geom3D/OGLRender/shaderprogramflat.cpp ../../TP_Geom3D/OGLRender/shaderprogramphong.cpp ../../TP_Geom3D/OGLRender/glew.c .tmp/OGLRender1.0.0/ && (cd `dirname .tmp/OGLRender1.0.0` && $(TAR) OGLRender1.0.0.tar OGLRender1.0.0 && $(COMPRESS) OGLRender1.0.0.tar) && $(MOVE) `dirname .tmp/OGLRender1.0.0`/OGLRender1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/OGLRender1.0.0


